#include<stdio.h>
#include<stdlib.h>

int main(int argc, char* argv[]) {
	printf("h\n");
	int** a;
	a = (int**) malloc(sizeof(int) * 3);
//	for (int i = 0; i < 3; i++) {
//		a[i]=i;
//	}
//	for (int i = 0; i < 3; i++) {
//		printf("%d", a[i]);
//	}
	for (int i = 0; i < 3; i++) {
		a[i] = (int*) malloc(sizeof(int) * 2);
	}
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 2; j++) {
			a[i][j] = i * 2 + j;
		}
	}
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 2; j++) {
			printf("%d", a[i][j]);
		}
		printf("\n");
	}
	return 0;
}
